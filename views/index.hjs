    <script>
        $('.special.cards .image').dimmer({
            on: 'hover'
        });

        $('.button[role="work__details"]').click(function(event) {
            var res_modal = $(this).parents('.card').data('card');
            console.log(res_modal);

            $('#'+res_modal)
                .modal({
                    blurring: true,
                    allowMultiple: false,
                })
                .modal('show');
        });
    </script>



    <script>
        $('.form.client').form({
            on: 'submit', //觸發條件 submit / blur / change
            //on: 'blur',
            inline: true, //prompt 出現方式
            fields:{
                name: {
                    identifier: 'name',
                    rules: [{
                        type: 'empty',
                        prompt: '請問要怎麼稱呼你⋯⋯？'
                    }]
                },
                email: {
                    identifier: 'email',
                    rules: [{
                        type: 'email',
                        prompt: '要輸入正確的信箱格式'
                    }]
                },
                message: {
                    identifier: 'msg',
                    rules: [{
                        type: 'empty',
                        prompt: '別忘記你的留言，也是很寶貴的'
                    }]
                }
            },
            onSuccess(event, fields){
                var clients = {}; //json 物件
                clients.name = $('#client__name').val(),
                clients.email = $('#client__email').val(),
                clients.msg = $('#client__message').val();
                $.post('/',clients ,function(data){
                    //回報前台 or somthing else
                    //傳了 json 給⋯⋯url('/') 
                });
                
            },
            onFailure(event, fields){
                //
            }
        })
    </script>
